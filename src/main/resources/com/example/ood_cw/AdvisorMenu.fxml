<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="AdvisorMenuAnchorID" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ood_cw.AdvisorMenu">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/BackGround2.jpg" />
         </image>
      </ImageView>
      <Button fx:id="AdvisorMenuScheduleEventButtonID" alignment="CENTER" layoutX="299.0" layoutY="120.0" mnemonicParsing="false" onAction="#AdvisorMenuScheduleEventButtonClick" prefHeight="50.0" prefWidth="300.0" text="Schedule Event">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
      <Button fx:id="AdvisorMenuReportingButtonID" layoutX="299.0" layoutY="420.0" mnemonicParsing="false" onAction="#AdvisorMenuReportingButtonClcik" prefHeight="50.0" prefWidth="300.0" text="Reporting">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
      <Button fx:id="AdvisorMenuClubCreationbuttonID" layoutX="299.0" layoutY="220.0" mnemonicParsing="false" onAction="#AdvisorMenuClubCreationButtonClick" prefHeight="50.0" prefWidth="300.0" text="Clubs">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
      <Button fx:id="AdvisorMenuAttendanceTrackingButtonID" layoutX="299.0" layoutY="320.0" mnemonicParsing="false" onAction="#AdvisorMenuAttendanceTrackingButtonClick" prefHeight="50.0" prefWidth="300.0" text="Attendance Tracking">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
      <Button fx:id="AdvisorMenuBackButtonID" alignment="BASELINE_LEFT" layoutY="30.0" mnemonicParsing="false" onAction="#AdvisorMenuBackButtonClick" prefHeight="60.0" prefWidth="110.0" style="-fx-background-radius: 0 60 60 0; -fx-border-color: black; -fx-border-radius: 0 60 60 0;" text="BACK">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
   </children>
</AnchorPane>
